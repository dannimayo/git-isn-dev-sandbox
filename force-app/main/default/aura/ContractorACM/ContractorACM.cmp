<aura:component implements="flexipage:availableForAllPageTypes,force:hasRecordId" controller="LoginController" access="global" >
	 <aura:attribute name="iframeUrl" type="String" />
    <aura:attribute name="width" type="String" />
    <aura:attribute name="height" type="String" />
    <aura:attribute name="scrolling" type="String" />
    <aura:attribute name="frameBorder" type="String" />
    <aura:attribute name="style" type="String" />
    <aura:attribute name="sandbox" type="String" />
    <aura:handler name="init" action="{!c.doInit}" value="{!this}"/>
	<aura:attribute name="response" type="String"/>
    <aura:attribute name="recordId" type="Id" />
       
<iframe src="{!v.response}"
            width="{!v.width}"
            height="1000px"
            frameBorder="auto"
            sandbox="{!v.sandbox}"
            scrolling="{!v.scrolling}"
            />  
</aura:component>