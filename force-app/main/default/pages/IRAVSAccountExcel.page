   <Column ss:Width="146.25"/>
   <Column ss:Width="125.25"/>
   <Column ss:Width="114.75"/>
   <Column ss:Width="147"/>
   <Column ss:Width="96.75"/>
   <Column ss:Width="72.75"/>
  <Row>
    <Cell ss:StyleID="s62"><Data ss:Type="String">I-RAVS Status</Data></Cell>
    <Cell ss:StyleID="s62"><Data ss:Type="String">Client Name</Data></Cell>
    <Cell ss:StyleID="s62"><Data ss:Type="String">Vertical Team</Data></Cell>
    <Cell ss:StyleID="s62"><Data ss:Type="String">Industry</Data></Cell>
    <Cell ss:StyleID="s62"><Data ss:Type="String">Country</Data></Cell>
    <Cell ss:StyleID="s62"><Data ss:Type="String">Sensitivity Rank</Data></Cell>
    <Cell ss:StyleID="s62"><Data ss:Type="String">Implementation Date</Data></Cell>
    <Cell ss:StyleID="s62"><Data ss:Type="String"># of Active Profiles</Data></Cell>
    <Cell ss:StyleID="s62"><Data ss:Type="String">Submission Acceptance Rate</Data></Cell>
    <Cell ss:StyleID="s62"><Data ss:Type="String">Overall Acceptance Rate</Data></Cell>
    <Cell ss:StyleID="s62"><Data ss:Type="String">Overall Rejection Rate</Data></Cell>
    <Cell ss:StyleID="s62"><Data ss:Type="String">Overall Non-Submission Rate</Data></Cell>
    <Cell ss:StyleID="s62"><Data ss:Type="String">Pending Variances</Data></Cell>
    <Cell ss:StyleID="s62"><Data ss:Type="String">Overall Grade</Data></Cell>
   </Row>
   <apex:repeat value="{!iravsClients}" var="i"> 
   <Row>
    <Cell ss:StyleID="s63"><Data ss:Type="String">{!i.IRAVSStatus__c}</Data></Cell>
    <Cell ss:StyleID="s63"><Data ss:Type="String">{!i.Name}</Data></Cell>
    <Cell ss:StyleID="s63"><Data ss:Type="String">{!i.Vertical__c}</Data></Cell>
    <Cell ss:StyleID="s63"><Data ss:Type="String">{!i.Industry__c}</Data></Cell>
    <Cell ss:StyleID="s63"><Data ss:Type="String">{!i.Country__c}</Data></Cell>
    <Cell ss:StyleID="s63"><Data ss:Type="String">{!i.Client_Sensitivity_Rank__c}</Data></Cell>
    <Cell ss:StyleID="s64"><Data ss:Type="DateTime"><apex:outputText value="{0,date,yyyy-MM-dd'T'00:00:00.000}"><apex:param value="{!i.ActDate__c}" /></apex:outputText></Data></Cell>
    <Cell ss:StyleID="s67"><Data ss:Type="Number">{!i.Active_Profiles__c}</Data></Cell>
    <Cell ss:StyleID="s65"><Data ss:Type="Number">{!Round(i.Submission_Acceptance_Rate__c,2)}</Data></Cell>
    <Cell ss:StyleID="s66"><Data ss:Type="Number">{!Round(i.Acceptance_Rate__c,2)}</Data></Cell>
    <Cell ss:StyleID="s65"><Data ss:Type="Number">{!Round(i.Rejection_Rate__c,2)}</Data></Cell>
    <Cell ss:StyleID="s65"><Data ss:Type="Number">{!Round(i.Non_Submission_Rate__c,2)}</Data></Cell>
    <Cell ss:StyleID="s67"><Data ss:Type="Number">{!i.Pending_Variances__c}</Data></Cell>
    <Cell ss:StyleID="s66"><Data ss:Type="String">{!i.Overall_Compliance_Grade__c}</Data></Cell>
   </Row>
   </apex:repeat>
  </Table>
