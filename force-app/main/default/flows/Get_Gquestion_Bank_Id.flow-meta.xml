<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Pass in variable formId to get questionBankId for latest published status Question Bank</description>
    <interviewLabel>Get Gquestion Bank Id {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Get Gquestion Bank Id</label>
    <processType>AutoLaunchedFlow</processType>
    <recordLookups>
        <name>Lookup_Current_Question_Bank_based_on_Form_ID</name>
        <label>Lookup Current Question Bank based on Form ID</label>
        <locationX>185</locationX>
        <locationY>235</locationY>
        <assignNullValuesIfNoRecordsFound>true</assignNullValuesIfNoRecordsFound>
        <filters>
            <field>Form__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>formId</elementReference>
            </value>
        </filters>
        <filters>
            <field>Status__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Published</stringValue>
            </value>
        </filters>
        <object>Question_Bank__c</object>
        <outputAssignments>
            <assignToReference>questionBankId</assignToReference>
            <field>Id</field>
        </outputAssignments>
        <sortField>Published_Date__c</sortField>
        <sortOrder>Desc</sortOrder>
    </recordLookups>
    <startElementReference>Lookup_Current_Question_Bank_based_on_Form_ID</startElementReference>
    <status>Active</status>
    <variables>
        <name>formId</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>true</isOutput>
    </variables>
    <variables>
        <name>questionBankId</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>true</isOutput>
    </variables>
</Flow>
