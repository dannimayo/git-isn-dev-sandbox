    <description>Launches Quick Action Flow for Team Members Leaving Voicemail</description>
    <formulas>
        <name>defaultCallDate</name>
        <dataType>Date</dataType>
        <expression>{!$Flow.CurrentDate} + 7</expression>
    </formulas>
    <formulas>
        <name>defaultTeam</name>
        <dataType>String</dataType>
        <expression>If({!currentUser.Activity_Team_Override__c}=True,TEXT({!currentUser.Team__c}) , {!thisAccount.Team__c})</expression>
    </formulas>
    <formulas>
        <name>recordTypeId</name>
        <dataType>String</dataType>
        <expression>IF(AND({!TEXT(thisAccount.ISNetworld_Status__c)} = &#39;Subscriber&#39;, OR({!TEXT(thisSite.Site_ISN_Status__c)} = &#39;Subscriber&#39;, {!thisSite.Id} = null)), &#39;01280000000BgKQ&#39;, 
IF(AND({!TEXT(thisAccount.ISNetworld_Status__c)} = &#39;Subscriber&#39;, {!TEXT(thisSite.Site_ISN_Status__c)} &lt;&gt;&#39;Subscriber&#39;, {!thisSite.Id} &lt;&gt; null),
&#39;01280000000BgKL&#39;,
&#39;01280000000BgKL&#39;))</expression>
    </formulas>
    <formulas>
        <name>shortened_desc</name>
        <dataType>String</dataType>
        <expression>IF(LEN({!Short_Description})&gt;250,Left({!Short_Description},200) &amp; &#39;(see description for more)&#39;,{!Short_Description})</expression>
    </formulas>
